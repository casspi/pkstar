    "axios": "^1.5.0",
    "copy-to-clipboard": "^3.3.3",
    "json5": "^2.2.3",
    "md5": "^2.3.0",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.0",
